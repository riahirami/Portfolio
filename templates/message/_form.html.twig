{{ form_start(form) }}
    <h1>form.html.twig</h1>

							
								<div class="form-floating mb-3">
				{{ form_label(form.utilisateur) }}
				{{ form_widget(form.utilisateur , { 'attr': { 'placeholder': 'nom d\'utilisateur...' ,'class': 'form-control'} } ) }}
								</div>
								<!-- Email address input-->
								<div class="form-floating mb-3">
													{{ form_label(form.email) }}
				{{ form_widget(form.email , { 'attr': { 'placeholder': 'email...' ,'class': 'form-control'} } ) }}

								</div>
								<!-- Phone number input-->
								<div class="form-floating mb-3">
													{{ form_label(form.telephone) }}
				{{ form_widget(form.telephone , { 'attr': { 'placeholder': 'telephone...' ,'class': 'form-control'} } ) }}
								</div>
								<!-- Message input-->
								<div class="form-floating mb-3">
													{{ form_label(form.message) }}
				{{ form_widget(form.message , { 'attr': { 'placeholder': 'ton message...' ,'class': 'form-control'} } ) }}
								</div>
								<!-- Submit success message-->
								<!---->
								<!-- This is what your users will see when the form-->
								<!-- has successfully submitted-->
									
								<!-- Submit error message-->
								<!---->
								<!-- This is what your users will see when there is-->
								<!-- an error submitting the form-->
									<div class="d-none" id="submitErrorMessage"> <div class="text-center text-danger mb-3">Error sending message!</div>
								</div>
								<!-- Submit Button-->
								<button class="btn btn-primary btn-xl ">{{ button_label|default('envoyer') }}</button>
{{ form_end(form) }}